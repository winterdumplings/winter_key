.container{
	padding-bottom: 0px;
	max-height: 100%;
}
.log_panel {
  position: fixed;
  color: rgb(4, 7, 8);
  width:300px;
  top: 5px;
  margin: 0px 30px;
  background-color: darkgray;
  padding: 10px;
}
.log_item{
  padding: 10px 5px;
  line-height:50rpx;
  color:rgb(27, 20, 20);
  border:3rpx solid #fff;
  border-radius: 20rpx;
  margin:0 auto 20rpx auto;
}
.log_item text {
  width: 100%;
  height: 100px;
  padding-left: 5px;
  
}
.clear{
	position: absolute;
	bottom: 0px;
	width: 100%;
  text-align: center;
}
.clear .clear_btn {
	margin: 10px;
	height: 45px;
  line-height: 35px;
  font-size: 30px;
  text-align: center;
	color:#fff;
	background-color: #7697d4;
	border-radius: 2px;
  width: 80%;
}
.nodata{
  position: fixed;
  color: rgb(4, 7, 8);
  width:300px;
  top: -80px;
  margin: 0px 30px;
  background-color: rgb(255, 255, 255);
  padding: 10px;
  text-align: center;
  font-size: 30px;
}